[global]
ioengine=sync
direct=1
fadvise_hint=0
verify=0
invalidate=1
file_append=0
create_on_open=1
end_fsync=1
thread=1
openfiles=1
group_reporting=1
allrandrepeat=1
filename_format=$jobname.$jobnum.$filenum
rw=write

[seq-write]
directory=${DIR}
numjobs=${NUM_JOB}
filesize=${FILESIZE}
nrfiles=${NRFILES}
bs=${BS}
