[global]
ioengine=libaio
direct=1
fadvise_hint=0
iodepth=64
invalidate=1
thread=1
openfiles=1
group_reporting=1
create_serialize=0
allrandrepeat=0
file_service_type=random
rw=randread
filename_format=$jobname.$jobnum.$filenum

[rand-read]
directory=${DIR}
bs=${BS}
filesize=${FILESIZE}
numjobs=${NUM_JOB}
nrfiles=${NRFILES}
