[global]
ioengine=libaio
direct=0
verify=0
bs=$BS
iodepth=$IO_DEPTH
runtime=120s
time_based=0
fadvise_hint=0
nrfiles=$NRFILES
thread=1
openfiles=1
group_reporting=1
filename_format=test.$jobnum.$filenum
file_service_type=roundrobin:200
number_ios=4000

[test]
rw=$IO_TYPE
filesize=$FILE_SIZE
directory=$TEST_DATA_DIR
numjobs=$THREADS
