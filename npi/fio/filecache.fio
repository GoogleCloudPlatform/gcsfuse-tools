[global]
allrandrepeat=1
group_reporting=1
iodepth=128
ioengine=libaio
rw=read
thread=1
time_based=0
invalidate=0
direct=0
stonewall
numjobs=${NUM_JOBS}
filename_format=read/${FILE_SIZE}/${BLOCK_SIZE}/${NR_FILES}/${NUM_JOBS}/experiment.$jobnum.$filenum

[read]
directory=${DIR}
nrfiles=${NR_FILES}
filesize=${FILE_SIZE}
bs=${BLOCK_SIZE}