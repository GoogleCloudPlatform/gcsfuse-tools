[global]
allrandrepeat=0
create_serialize=0
direct=1
fadvise_hint=0
file_service_type=random
group_reporting=1
iodepth=${QUEUE_DEPTH}
ioengine=libaio
invalidate=1
openfiles=1
rw=${IO_TYPE}
thread=1
time_based=1
ramp_time=30s
runtime=2m
filename_format=fullsweep/${FILE_SIZE}/${NR_FILES}/${IO_TYPE}/${NUM_JOBS}/${BLOCK_SIZE}/experiment.$jobnum$filenum

[experiment]
stonewall
directory=${DIR}
bs=${BLOCK_SIZE}
filesize=${FILE_SIZE}
nrfiles=${NR_FILES}
numjobs=${NUM_JOBS}