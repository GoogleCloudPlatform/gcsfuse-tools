[global]
ioengine=libaio
direct=1
fadvise_hint=0
iodepth=64
invalidate=1
nrfiles=${NR_FILES}
thread=1
openfiles=1
group_reporting=1
create_serialize=0
allrandrepeat=0
file_service_type=random
numjobs=${NUM_JOBS}
filename_format=$jobname.$jobnum/$filenum

[Workload]
directory=${DIR}
bs=${BLOCK_SIZE}
filesize=${FILE_SIZE}
rw=${READ_TYPE}
