[global]
ioengine=libaio
direct=1
fadvise_hint=0
iodepth=64
invalidate=1
thread=1
openfiles=1
group_reporting=1
create_serialize=0
allrandrepeat=0
file_service_type=random
numjobs=16
filename_format=$jobname.$jobnum/$filenum

[random_read_filesize_1M_blocksize_1M]
stonewall
directory=${DIR}
bs=1M
filesize=1MB
rw=randread
nrfiles=30
